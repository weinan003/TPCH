select
 sum(l_extendedprice * l_discount) as revenue
from
 lineitem
where
 l_shipdate >= date '1997-01-01'
 and l_shipdate < date '1997-01-01' + interval '1 year'
 and l_discount between 0.06 - 0.01 and 0.06 + 0.01
 and l_quantity < 24;select '***', 'tpch_parquet_1gpn_nocomp_nopart_wwang_1_1_TPCH_Query_06.sql_hawq', sess_id from pg_stat_activity where current_query like '%tpch_parquet_1gpn_nocomp_nopart_wwang_1_1_TPCH_Query_06.sql_hawq%';